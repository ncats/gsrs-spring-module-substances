<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender" >
        <encoder>
            <pattern>%d [%thread] %-5level  %logger{35} - %msg%n</pattern>
        </encoder>
    </appender>

    
    <root level="ERROR">
        <appender-ref ref="STDOUT" />
    </root>

    <logger name="example.chem" level="TRACE" additivity="false">
        <appender-ref ref="STDOUT" />
    </logger>
</configuration>
